Fun)
    ConsoleWrite($i)
EonFunc[

Func default_plus_expression($val = 3 + 3)
    ConsoleWrite($val)
EndFut_int()
; expect-stdout: "123"
default_int(123)

; expect-stdout: "I Am a String"
default_string()
; expect-stdout: "A string"
default_string("A string")

; expect-stdout: "7"
default_plus_expression()
; expect-stdout: "19"
def_expression(19)